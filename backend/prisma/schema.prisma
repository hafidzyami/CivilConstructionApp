
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum AnalysisStatus {
  PENDING
  PROCESSING
  COMPLETED
  FAILED
}

model Detail {
  id          Int            @id @default(autoincrement())
  latitude    Float
  longitude   Float
  address     String?
  environment Json?
  decision    String?
  explanation String?
  status      AnalysisStatus @default(PENDING)
  createdAt   DateTime       @default(now())
  updatedAt   DateTime       @updatedAt
  deletedAt   DateTime?
}
